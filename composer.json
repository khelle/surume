{
    "name": "khelle/surume",
    "description": "Surume webapplication framework.",
    "keywords": [ "surume" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kamil Jamroz"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/console": "2.6.*",
        "symfony/http-foundation": "3.0.*",
        "league/flysystem": "1.0.*",
        "league/container": "1.3.*",
        "monolog/monolog": "1.13.*",
        "guzzlehttp/guzzle": "6.1.*",
        "cboden/ratchet": "0.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*|5.2.*"
    },
    "suggest": {
        "ext-libevent": ">=0.1.0",
        "ext-event": "~1.0",
        "ext-libev": "*"
    },
    "autoload": {
        "psr-4": {
            "Surume\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Surume\\Test\\": "test"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
